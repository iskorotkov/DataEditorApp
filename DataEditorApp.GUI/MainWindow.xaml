<Window x:Class="DataEditorApp.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataEditorApp.GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <TabControl x:Name="tabControl">
        <TabItem x:Name="AddTab" Header="Add">
            <Frame x:Name="AddFrame" Content="" NavigationUIVisibility="Hidden" Source="/AddPage.xaml"/>
        </TabItem>
        <TabItem x:Name="ViewTab" Header="View">
            <Frame x:Name="ViewFrame" Content="" NavigationUIVisibility="Hidden" Source="/ViewPage.xaml"/>
        </TabItem>
        <TabItem x:Name="EditTab" Header="Edit">
            <Grid Background="#FFE5E5E5"/>
        </TabItem>
        <TabItem x:Name="DeleteTab" Header="Delete">
            <Grid Background="#FFE5E5E5"/>
        </TabItem>
    </TabControl>
</Window>
