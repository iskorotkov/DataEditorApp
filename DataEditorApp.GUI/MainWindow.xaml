<Window x:Class="DataEditorApp.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataEditorApp.GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Menu x:Name="menu">
            <MenuItem x:Name="EditMenu" Header="Edit">
                <MenuItem x:Name="AddUser" Header="Add" Click="AddUser_Click"/>
                <MenuItem x:Name="ModifyUser" Header="Modify" Click="ModifyUser_OnClick"/>
                <MenuItem x:Name="DeleteUsers" Header="Delete"/>
            </MenuItem>
        </Menu>
        <Frame Name="MainFrame" Content="" NavigationUIVisibility="Hidden"/>
    </StackPanel>
</Window>
